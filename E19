
class Solution {
    public int searchInsert(int[] nums, int target) {
        int len = nums.length;
        int i =0;
        for (i = 0; i<=len-1; i++){
            if(nums[i]==target){
                return i;
            } else if(target < nums[i]){
                return i;
            }
        }
        return i;
    }
}
