class Solution {
    public void moveZeroes(int[] nums) {
        int len = nums.length-1;
        for (int i = 0; i<=len; i++){
            if (nums[i] == 0){
                int j =i;
                while(j<len && nums[j] == 0){
                    if (nums[j] == 0){
                        j++;
                    }
                }
                nums[i]= nums[j];
                nums[j]=0;
            }
        }
    }
}
